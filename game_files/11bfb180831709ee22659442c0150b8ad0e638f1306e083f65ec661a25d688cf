function addTop(a){var c={};var b=0;a.state.add("top",{preload:function(){},create:function(){c.bg=a.add.tileSprite(0,0,640,832,"top-bg");c.button=a.add.image(320,700,"top-start");c.button.anchor.set(0.5);a.add.tween(c.button.scale).to({x:1.07,y:1.07},1000,Phaser.Easing.Linear.None,true,0,-1,true);c.topText=a.add.text(320,780,"※サウンドが再生されます",{fontSize:"28px",fill:"#000",stroke:"#fff",strokeThickness:10});c.topText.anchor.set(0.5);a.input.onDown.add(function(d){a.state.start("stage")},a)},update:function(){}})};